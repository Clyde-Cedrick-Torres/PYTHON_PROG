    "\n",
    "\n",
    "length = len(word)\n",
    "\n",
    "\n",
    "if length < 5:\n",
    "    print(\"This word has less than 5 characters.\")\n",
    "elif length == 5:\n",
    "    print(\"This word has exactly 5 characters.\")\n",
    "else:\n",
    "    print(\"This word has more than 5 characters.\")"
